using namespace std
#include <iostream>
int main(int argc, char const *argv[])
{
    
     int size;
    cout<<"enter the sier of the array";
    cin>>size;
    int arr[size];
    for(int i=0; i<size; i++){
        cin>>arr[i];
    }
    string result;
    for(int i=0; i<size / 2; i++){
        for(int j=size; j<size / 2; j--){
            if(arr[i] == arr[j]){
                result = "Yes";
            }else{
                result = "NO";
            }
        }
    }
    
    cout<<result;
    return 0;
}
